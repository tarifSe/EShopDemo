@model int


<td style="width:150px">
    <div class="btn-group">
        <a asp-action="Edit" asp-controller="ProductTypes" class="btn btn-warning" asp-route-id="@Model"><i class="fa-regular fa-pen-to-square"></i></a>
        <a asp-action="Details" asp-controller="ProductTypes" class="btn btn-success" asp-route-id="@Model"><i class="fa fa-list"></i></a>
        <a asp-action="Delete" asp-controller="ProductTypes" class="btn btn-danger" asp-route-id="@Model"><i class="fa-regular fa-trash-can"></i></a>
    </div>
</td>
@model string


<td style="width:150px">
    <div class="btn-group">
        <a asp-action="Edit" class="btn btn-warning" asp-route-id="@Model"><i class="fa-regular fa-pen-to-square"></i></a>
        <a asp-action="Details" class="btn btn-success" asp-route-id="@Model"><i class="fa fa-list"></i></a>
